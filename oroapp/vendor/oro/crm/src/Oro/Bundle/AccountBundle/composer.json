    "homepage": "https://github.com/orocrm/AccountBundle",
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "friendsofsymfony/rest-bundle": "1.5.0-RC2",
