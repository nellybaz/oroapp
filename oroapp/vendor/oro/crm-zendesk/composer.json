{
    "name": "oro/crm-zendesk",
    "description": "OroCRM Zendesk Integration",
    "homepage": "https://github.com/oroinc/OroCRMZendeskBundle.git",
    "license": "OSL-3.0",
    "keywords": ["Oro", "Zendesk", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-4": {"Oro\\Bundle\\ZendeskBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "php": "7.1.*",
        "oro/crm": "2.6.*"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        }
    }
}
