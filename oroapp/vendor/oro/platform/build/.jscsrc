{
  "preset": "google",
  "excludeFiles": [
    "../../**/node_modules/**",
    "../../**/public/lib/**",
    "../../**/public/*/vendors/**",
    "../../**/Karma/lib/**",
    "../../**/*.min.js"
  ],
  "validateIndentation": 4,
  "maximumLineLength": {
	 "value": 120,
	 "tabSize": 4,
	 "allowComments": true,
	 "allowRegex": true
  },
  "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
  "disallowSpaceAfterObjectKeys": true,
  "requireSpaceBeforeObjectValues": true,
  "validateLineBreaks": "LF",
  "validateParameterSeparator": ", "
}
