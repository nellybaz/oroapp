{
    "name": "oro/dashboard-bundle",
    "type": "symfony-bundle",
    "description": "BAP Dashboard Bundle",
    "keywords": ["BAP", "Dashboard"],
    "license": "MIT",
    "require": {
        "php": "7.1.*",
        "symfony/symfony": "2.8.*, !=2.8.10",
        "oro/config": "dev-master",
        "oro/ui-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/security-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\DashboardBundle": "" }
    },
    "target-dir": "Oro/Bundle/DashboardBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
