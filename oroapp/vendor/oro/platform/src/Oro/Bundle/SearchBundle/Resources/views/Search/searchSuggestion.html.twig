{% spaceless %}
<ul data-count={% if records_count is defined %}{{ records_count }}{% else %}0{% endif %}>
    {% if data is defined %}
        {% for item in data %}
        <li>
            <a href="{{ item.record_url }}">
                <span class="name"><strong>{{ item.record_string }}</strong></span>
                {{ oro_entity_config_value(item.entity_name, 'label')|trans }}
            </a>
        </li>
        {% endfor %}
    {% endif %}
</ul>
{% endspaceless %}
