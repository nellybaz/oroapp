    <span><a class="hash-navigation-link" href="<%= url %>"><%= subject %></a> {{ "oro.reminder.reminder_template.due_message"|trans }} <%= expireAt %></span>
    <% } else { %>
    <span><%= subject %>{{ "oro.reminder.reminder_template.due_message"|trans }} <%= expireAt %></span>
    <% } %>
</script>
