{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'customer-customer-user-grid' %}
{% set pageTitle = 'oro.customer.customeruser.entity_plural_label'|trans %}

{% block navButtons %}
    {% if is_granted('oro_customer_customer_user_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('oro_customer_customer_user_create'),
                'entity_label': 'oro.customer.customeruser.entity_label'|trans
            }) }}
        </div>
    {% endif %}

    {% include 'OroImportExportBundle:ImportExport:buttons_from_configuration.html.twig' with {
        'alias': 'oro_customer_user'
    } %}

{% endblock %}
