<div class="control-group">
    <label class="control-label">{{ 'oro.payment.methods.label'|trans }}</label>
    <div class="controls">
        <div class="control-label">
            {%- if data is not empty and data is iterable -%}
                {% for paymentMethod in data %}
                    {{ get_payment_method_admin_label(paymentMethod)|oro_html_sanitize }}{% if not loop.last %},{% endif %}
                {% endfor %}
            {%- else -%}
                {{ 'oro.ui.empty'|trans }}
            {%- endif -%}
        </div>
    </div>
</div>
