{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set pageTitle = 'oro.product.brand.menu.entity_plural_label'|trans %}

{% block navButtons %}
    {% if resource_granted('oro_product_brand_create') %}
        <div class="btn-group">
            {{ UI.addButton({
                'path': path('oro_product_brand_create'),
                'entity_label': 'oro.product.brand.entity_label'|trans})
            }}
        </div>
    {% endif %}
    {{ parent() }}
{% endblock navButtons %}
